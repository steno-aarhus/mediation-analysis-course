	month = feb,
	year = {2022},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Predictive markers, Cardiovascular diseases, High-throughput screening, Metabolic disorders},
	pages = {936},
	file = {Full Text PDF:C\:\\Users\\au302898\\Zotero\\storage\\T8FPAUX7\\Wittenbecher et al. - 2022 - Dihydroceramide- and ceramide-profiling provides i.pdf:application/pdf},
